#include <iostream>
#include <stdio.h>
#include <cstdlib>

using namespace std;

int main()
{
    system("chcp 1251>nul");
    int n = 10, k = 2;
    
    while (k <= n)
    {
        if (k%2 == 0)
        {
            cout << k << endl;
            
        }
        k++;
    }
    system("pause>nul");
    return 0;
}
